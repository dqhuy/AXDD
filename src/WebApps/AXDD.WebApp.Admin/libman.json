{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.7.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js",
        "jquery.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validation/",
      "files": [
        "jquery.validate.min.js",
        "jquery.validate.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js",
        "jquery.validate.unobtrusive.js"
      ]
    },
    {
      "library": "bootstrap@4.6.2",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "css/bootstrap.min.css",
        "css/bootstrap.css",
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.js"
      ]
    },
    {
      "library": "font-awesome@6.4.2",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css",
        "webfonts/"
      ]
    },
    {
      "library": "admin-lte@3.2.0",
      "destination": "wwwroot/lib/admin-lte/",
      "files": [
        "css/adminlte.min.css",
        "css/adminlte.css",
        "js/adminlte.min.js",
        "js/adminlte.js"
      ]
    },
    {
      "library": "datatables@1.13.6",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "css/jquery.dataTables.min.css",
        "js/jquery.dataTables.min.js"
      ]
    },
    {
      "library": "datatables.net-bs4@1.13.6",
      "destination": "wwwroot/lib/datatables.net-bs4/",
      "files": [
        "css/dataTables.bootstrap4.min.css",
        "js/dataTables.bootstrap4.min.js"
      ]
    },
    {
      "library": "chart.js@4.4.0",
      "destination": "wwwroot/lib/chart.js/",
      "files": [
        "chart.umd.min.js",
        "chart.umd.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@7.0.14",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.js"
      ]
    }
  ]
}
